<%= turbo_frame_tag dom_id(@post) do %>
    <article class="card">
        <div class="card-body">
            <h5>Editing <%= @post.title %></h5>
            <%= render 'form', post: @post %>

            <%= link_to 'Cancel', @post, class: 'btn btn-outline-primary' %>
        </div>
    </article>
<% end %>
